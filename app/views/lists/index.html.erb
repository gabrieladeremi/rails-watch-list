<p>List</p>

<ul>
  <% @lists.each do |list| %>
    <li><%= list.name %></li>
    <br />
    <%= link_to  list_path(list, @bookmark) do%>
      <button type="button" class="btn btn btn-outline-info m-3">View</button>
    <% end %>
  <% end %>
</ul>

 <%= link_to new_list_path do%>
    <button type="button" class="btn btn-primary m-3">Add List</button>
  <% end %>
